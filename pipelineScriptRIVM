stages {  
  stage ('Run JMeter Test') {
  sh '/opt/apache-jmeter-3.2/bin/jmeter.sh -Jjmeter.save.saveservice.output_format=xml -n -t /opt/loadtest/demo.sdv.rivm.nl/samenmeten.jmx -l samenmeten.jtl'
  }
  stage ('Publish Artifacts'){
 echo 'Test Started' 
  }
  stage ('Test Finished'){
  echo 'Test Finished' 	
 }
 }